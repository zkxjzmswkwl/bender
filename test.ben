class Player {
    ctor(name) {
        this.name = name;
        this.hp = 100;
    }

    str() {
        return "Player: " + this.name;
    }

    takeDamage(amount) {
        this.hp = this.hp - amount;
    }

    heal(amount) {
        this.hp = this.hp + amount;
    }
}

val p = Player("Carter");
print p.str();
print p.hp;
p.takeDamage(50);
print p.hp;
